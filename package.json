{
  "name": "doc-template",
  "displayName": "doc_template",
  "description": "",
  "publisher": "hebrown",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:markdown"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "template.showTemplates",
        "title": "Choose a Template",
        "category": "Doc Templates"
      },
      {
        "command": "template.gettingstarted",
        "title": "Getting Started",
        "enablement": "inQuickOpen != true"
      },
      {
        "command": "template.howto",
        "title": "How-to",
        "enablement": "inQuickOpen != true"
      },
      {
        "command": "template.faq",
        "title": "FAQ",
        "enablement": "inQuickOpen != true"
      },
      {
        "command": "template.developerguide",
        "title": "Developer Guide",
        "enablement": "inQuickOpen != true"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "submenu": "template.template",
          "group": "z_commands",
          "when": "explorerResourceIsFolder && explorerResourceIsRootFolder != true"
        }
      ],
      "template.template": [
        {
          "command": "template.gettingstarted"
        },
        {
          "command": "template.howto"
        },
        {
          "command": "template.faq"
        },
        {
          "command": "template.developerguide"
        }
      ]
    },
    "submenus": [
      {
        "id": "template.template",
        "label": "Add a template"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.76.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5",
    "@vscode/test-electron": "^2.2.3"
  }
}